<html>
    <head>
        <title>Year: {{year}}</title>
    </head>
    <body>
        <h1>CSL {{year}}</h1>
        <ul>
            {{#each teams}}
                <li class="{{#if @first}}champion{{/if}}">
                    {{@index}}. <b>{{name}}</b>: {{pts}}
                </li>
            {{/each}}
        </ul>
    </body>
</html>